/dts-v1/;

#include "imx6q.dtsi"
#include "icorem6qdl.dtsi"

/ {
	model = "Engicam i.CoreM6 Quad/Dual resistive openframe";
	compatible = "fsl,imx6-icore", "fsl,imx6q";
};

&ldb {
	ipu_id = <0>;
	sec_ipu_id = <0>;
};

&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart3 {
	fsl,uart-has-rtscts;
	status = "okay";
};

&flexcan2 {
	status = "okay";
};

&i2c1 {

	max11801@48 {
		compatible = "maxim,max11801";
		reg = <0x48>;
		interrupt-parent = <&gpio3>;
		interrupts = <31 2>;
		work-mode = <0>;/*DCM mode*/
	};

};

&i2c3 {
	pcf8563: rtc@51 {
		compatible = "pcf8563";
		reg = <0x51>;
	};
};
