/dts-v1/;

#include "imx6q.dtsi"
#include "icorem6qdl.dtsi"

/ {
	model = "Engicam i.CoreM6 Quad/Dual resistive openframe";
	compatible = "fsl,imx6-icore", "fsl,imx6q";

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;
		GPIO_DL2 {
		        gpio-export,name = "GPIO_DL2";
			gpio-export,output = <1>;
                        gpio-export,direction_may_change;
		        gpios = <&gpio1 3 0>;
		};
	};
};

&ldb {
	ipu_id = <0>;
	sec_ipu_id = <0>;
};

&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart3 {
	fsl,uart-has-rtscts;
	status = "okay";
};

&flexcan2 {
	status = "okay";
};

&i2c1 {

	max11801@48 {
		compatible = "maxim,max11801";
		reg = <0x48>;
		interrupt-parent = <&gpio3>;
		interrupts = <31 2>;
		work-mode = <0>;/*DCM mode*/
	};

};

&i2c3 {
	pcf8563: rtc@51 {
		compatible = "pcf8563";
		reg = <0x51>;
	};
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;
	hog {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_3__GPIO1_IO03 0x1f059 /* LED DL2*/
			>;
		};
	};
};
